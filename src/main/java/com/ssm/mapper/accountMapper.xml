<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ssm.dao.AccountDao">
    <select id="selectAll" resultType="com.ssm.pojo.User">
        select * from java.t_user where 1=1
    </select>
    <insert id="accountLogin" parameterType="com.ssm.pojo.User">
        insert into java.t_user(id,create_time,update_time,is_deleted,user_id,account,real_name,password) value (null,create_time,update_time,0,user_id,account,real_name,password);
    </insert>
</mapper>
